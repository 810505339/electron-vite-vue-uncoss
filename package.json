    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.12.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.1.7",
    "@types/uuid": "^9.0.1"
  },
  "dependencies": {
    "@unocss/reset": "^0.50.1",
